<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="InterButton" parent="@android:style/Theme.NoTitleBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">#E95634</item>
    </style>
    <style name="VerticalGravityCenter" parent="@android:style/Theme.NoTitleBar">
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="AdName" parent="VerticalGravityCenter">
        <item name="android:background">#ebebeb</item>
        <item name="android:textColor">#363636</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">17dp</item>
        <item name="android:paddingLeft">6dp</item>
    </style>
    <style name="AdStatus" parent="VerticalGravityCenter">
        <item name="android:background">#ccc6c7</item>
        <item name="android:textColor">#292929</item>
        <item name="android:textSize">14dp</item>
        <item name="android:paddingLeft">6dp</item>
    </style>
</resources>
